    "next": "^15.0.3",
    "next-auth": "^5.0.0-beta.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.4",
    "superjson": "^2.2.5",
    "tailwind-merge": "^2.5.5",
